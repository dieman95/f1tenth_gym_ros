<?xml version="1.0"?>
<launch>
  
  <!-- Launch a map from the maps folder-->
  <arg name="map" default="$(find f1tenth_gym_ros)/maps/unreal.yaml"/>
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map)"/>

  <!-- Launch robot model -->
  <include file="$(find f1tenth_gym_ros)/launch/racecar_model.launch"/>

  <!-- Launch gym bridge node -->
  <rosparam command="load" file="$(find f1tenth_gym_ros)/params.yaml"/>
  <node pkg="f1tenth_gym_ros" name="gym_bridge" type="gym_bridge.py" output="screen"/>

  <!--launch the safety node-->
  <node pkg="f1tenth_gym_ros" name="safety_node_ego" type="safety_node.py" output="screen"/>
  <!--launch the disparity extender-->
  <node pkg="f1tenth_gym_ros" name="disparity_extender_ego" type="disparity_extender.py" args="scan drive_cmd ego_racecar/laser" output="screen"/>
  <!-- launch the decision manager node-->
  <node pkg="f1tenth_gym_ros" name="decision_manager_ego" type="decision_manager.py" args="brake_bool drive_cmd drive" output="screen"/>

</launch>
